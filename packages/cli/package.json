{
  "name": "@thorgate/i18n-cli",
  "version": "1.0.0-alpha1",
  "description": "CLI for i18n utilities",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thorgate/tg-i18n.git"
  },
  "keywords": [
    "tg-i18n",
    "react",
    "react-dom",
    "i18n",
    "translations",
    "gettext",
    "higher order component",
    "hoc"
  ],
  "bin": {
    "node-compilemessages": "./bin/compilemessages.js",
    "node-makemessages": "./bin/makemessages.js",
    "node-msgmerge": "./bin/msgmerge.js"
  },
  "author": "Thorgate <hi@thorgate.eu>",
  "contributors": [
    "Jürno Ader <jyrno42@gmail.com> (https://github.com/Jyrno42)",
    "Jörgen Ader <jorgen.ader@gmail.com> (https://github.com/metsavaht)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thorgate/tg-i18n/issues"
  },
  "homepage": "https://github.com/thorgate/tg-i18n",
  "devDependencies": {
    "coveralls": "3.0.2",
    "jest-cli": "23.6.0",
    "rimraf": "*",
    "rollup": "0.66.2",
    "ts-jest": "23.10.3",
    "tslint": "5.11.0",
    "typescript": "3.0.3"
  },
  "dependencies": {
    "@thorgate/node-compilemessages": "1.0.0-alpha1",
    "@thorgate/node-makemessages": "1.0.0-alpha1",
    "@thorgate/node-msgmerge": "1.0.0-alpha1",
    "tslib": "1.9.3",
    "yargs": "12.0.2"
  },
  "files": [
    "bin/"
  ],
  "scripts": {
    "clean": "rimraf bin",
    "lint": "tslint -p tsconfig.lint.json",
    "prebuild": "yarn clean -s && yarn lint",
    "build": "rollup -c=../../rollup.config.js && rimraf lib && chmod +x -R bin/",
    "prepublish": "yarn build",
    "prepare": "yarn build -s",
    "prepublishOnly": "yarn build -s"
  }
}
